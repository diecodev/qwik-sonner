{
  "name": "qwik-sonner",
  "version": "0.3.2",
  "description": "An opinionated toast component for Qwik.",
  "keywords": [
    "qwik",
    "qwik library",
    "typescript",
    "toast",
    "qwik sonner",
    "sonner"
  ],
  "author": {
    "name": "diecodev",
    "url": "https://github.com/diecodev"
  },
  "contributors": [
    {
      "name": "devalexpy",
      "url": "https://github.com/devalexpy"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/diecodev/qwik-sonner"
  },
  "main": "index.qwik.mjs",
  "qwik": "index.qwik.mjs",
  "types": "index.d.ts",
  "files": [
    "headless",
    "toaster-styled.d.ts",
    "index.qiwk.mjs",
    "index.qiwk.cjs",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./index.qwik.mjs",
      "require": "./index.qwik.cjs",
      "types": "./index.d.ts"
    },
    "./headless": {
      "import": "./headless/index.qwik.mjs",
      "require": "./headless/index.qwik.cjs",
      "types": "./headless/index.d.ts"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "private": false,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "bumpp --commit --tag --push && npm run build && npm publish",
    "postpublish": "rm -rf headless index.d.ts index.qwik.cjs index.qwik.mjs toaster-styled.d.ts",
    "start": "vite --open --mode ssr",
    "test": "echo \"No test specified\" && exit 0",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.5.1",
    "@types/eslint": "8.56.7",
    "@types/node": "20.12.2",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "bumpp": "9.4.0",
    "eslint": "8.57.0",
    "eslint-plugin-qwik": "1.5.1",
    "prettier": "3.2.5",
    "tsc-alias": "1.8.8",
    "typescript": "5.4.3",
    "undici": "6.10.2",
    "vite": "5.2.7",
    "vite-plugin-dts": "3.8.1",
    "vite-tsconfig-paths": "4.3.2"
  }
}
